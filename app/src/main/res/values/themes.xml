<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AppConsultoria" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/nave_bar_color_backgraound</item>
        <item name="colorPrimaryDark">@color/nave_bar_color_backgraound</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/nave_bar_color_backgraound</item>
        <item name="colorSecondaryVariant">@color/nave_bar_color_backgraound</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/nave_bar_color_backgraound_gradent</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AppConsultoria.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AppConsultoria.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AppConsultoria.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>